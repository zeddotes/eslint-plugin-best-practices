{
  "name": "eslint-plugin-best-practices",
  "version": "1.2.1",
  "description": "ESLint plugin that enforces best practices in JavaScript/TypeScript codebases",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run clean && npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "release": "standard-version",
    "release:alpha": "standard-version --prerelease alpha",
    "release:beta": "standard-version --prerelease beta",
    "release:rc": "standard-version --prerelease rc"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "eslint-best-practices",
    "typescript"
  ],
  "author": {
    "name": "Zain",
    "url": "https://github.com/zeddotes"
  },
  "license": "ISC",
  "dependencies": {
    "@typescript-eslint/rule-tester": "^8.32.0",
    "@typescript-eslint/utils": "^8.32.0",
    "path": "^0.12.7",
    "url": "^0.11.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "eslint": "^9.26.0",
    "eslint-plugin-prettier": "^5.4.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.5",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zeddotes/eslint-plugin-best-practices.git"
  },
  "bugs": {
    "url": "https://github.com/zeddotes/eslint-plugin-best-practices/issues"
  },
  "homepage": "https://github.com/zeddotes/eslint-plugin-best-practices#readme"
}
